{
  "configurations": [
    {
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/${config:azureFunctions.deploySubpath}"
      },
      "justMyCode": false,
      "name": "Current File (Integrated Terminal)",
      "program": "${file}",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "connect": {
        "host": "localhost",
        "port": 9091
      },
      "name": "Attach to Python Functions",
      "preLaunchTask": "func: host start",
      "request": "attach",
      "type": "debugpy"
    }
  ],
  "version": "0.2.0"
}
